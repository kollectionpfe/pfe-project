{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "functions": {
 
    "dist/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }


}