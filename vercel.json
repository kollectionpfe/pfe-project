{
    "version": 2,
    "builds": [
      {
        "src": "api/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/main.js"
      }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "api"


}